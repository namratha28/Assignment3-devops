@{
    ViewData["Title"] = "Second Song";
}

<head>
    <meta charset="utf-8" />
    <script
        src="https://code.jquery.com/jquery-3.5.1.min.js"
        integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script>
        $(function () {
            $('#btn-all').on('click', function () {
                console.log("getting..");
                $.get(
                    'http://localhost:5000/Music/SecondSong',
                    function (response) {
                        console.log("> ", response);
                        $("#result").html(response);
                });
            });
        });
    </script>
</head>

<body>
    <button id="btn-all">Second Song</button>
    <br />
    <div id="result"></div>
</body>
